import json
import urllib2
import time

while True:
    time.wait(1000)
    #get temperature in F
    print("Temperature(F) is " + str(json.load(urllib2.urlopen("http://192.168.1.18/temp"))))

    print ("Temperature(C) is " + str(json.load(urllib2.urlopen("http://192.168.1.18/temp_c"))))
    
    
